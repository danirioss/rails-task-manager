<h1>Create a new Task</h1>

<%#= form_with model: @task do |f| %>

<%= form_with model: @task do |f| %>
  <%= f.label "Title:" %>
  <%= f.text_field :title %>
  <%= f.label "Details:" %>
  <%= f.text_area :details %>
  <%= f.submit "Create Task" %>
<% end %>


<%= link_to "Back to list", tasks_path %>
